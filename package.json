{
  "name": "accounts-ui",
  "version": "1.0.0",
  "description": "Zesty.io account management application",
  "repository": {
    "git": "https://github.com/zesty-io/accounts-ui"
  },
  "main": "webpack.config.js",
  "scripts": {
    "prebuild": "node bin/preBuild",
    "build": "node bin/build",
    "prestart": "npm install & npm run prebuild",
    "start": "node bin/start",
    "serve": "./node_modules/.bin/serve ./build --port 6006 --single",
    "build-prod": "export NODE_ENV=\"production\" && npm run build",
    "build-dev": "export NODE_ENV=\"development\" && npm run build",
    "build-stage": "export NODE_ENV=\"staging\" && npm run build"
  },
  "author": "Stuart Runyan<stuart@zesty.io>",
  "contributors": [
    "Grant Glidewell <grant.glidewell@zesty.io>"
  ],
  "license": "",
  "dependencies": {
    "classnames": "2.2.5",
    "qs": "^6.5.1",
    "react": "16.2.0",
    "react-dom": "16.2.0",
    "react-redux": "5.0.7",
    "react-router": "4.2.0",
    "react-router-dom": "4.2.2",
    "redux": "3.7.2",
    "redux-thunk": "2.2.0"
  },
  "devDependencies": {
    "ava": "0.25.0",
    "babel-cli": "6.26.0",
    "babel-loader": "7.1.4",
    "babel-preset-es2015": "6.24.1",
    "babel-preset-react": "6.24.1",
    "babel-preset-stage-2": "6.24.1",
    "css-loader": "0.28.10",
    "extract-text-webpack-plugin": "3.0.2",
    "less": "^3.0.1",
    "less-loader": "4.1.0",
    "redux-logger": "3.0.6",
    "serve": "6.5.2",
    "standard": "11.0.0",
    "style-loader": "0.20.3",
    "webpack": "3.8.1"
  },
  "false": {}
}
