{
  "name": "accounts-app",
  "version": "1.0.0",
  "description": "Zesty.io account management applicaiton",
  "main": "index.js",
  "scripts": {
    "prestart": "npm prune & npm install",
    "start": "npm run build:dev",
    "build": "node bin/build",
    "build:dev": "export NODE_ENV=\"development\" && ./node_modules/.bin/webpack --watch & npm run serve",
    "build:prod": "export NODE_ENV=\"production\" && ./node_modules/.bin/webpack --progress -p",
    "serve": "./node_modules/.bin/serve ./build --port 9000 --single",
    "test": "./node_modules/.bin/ava tests"
  },
  "author": "Stuart Runyan<stuart@zesty.io>",
  "license": "UNLICENSED",
  "dependencies": {
    "classnames": "2.2.5",
    "react": "16.0.0",
    "react-dom": "16.0.0",
    "react-redux": "5.0.6",
    "react-router": "4.2.0",
    "react-router-dom": "4.2.2",
    "redux": "3.7.2",
    "redux-thunk": "2.2.0"
  },
  "devDependencies": {
    "ava": "0.22.0",
    "babel-cli": "6.26.0",
    "babel-loader": "7.1.2",
    "babel-preset-es2015": "6.24.1",
    "babel-preset-react": "6.24.1",
    "babel-preset-stage-2": "6.24.1",
    "css-loader": "0.28.7",
    "extract-text-webpack-plugin": "3.0.1",
    "less": "2.7.2",
    "less-loader": "4.0.4",
    "redux-logger": "3.0.6",
    "serve": "6.3.1",
    "style-loader": "0.19.0",
    "webpack": "3.8.1"
  },
  "false": {}
}
